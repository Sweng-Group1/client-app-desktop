<?xml version="1.0" encoding="UTF-8"?>

<presentation xmlns="urn:SWENG"
    xmlns:SWENG="https://raw.githubusercontent.com/SWENG-G2/xml_standard/main/standard.xsd">
    <info>
        <title>Evaluation presentation</title>
        <author>The Penelope Team</author>
        <date>2023-02-06</date>
        <numSlides>2</numSlides>
    </info>
    <slide width="500" height="500" title="1">
        <text fontName="mono" fontSize="32" colour="#7D13CFFF" xCoordinate="45" yCoordinate="45"
            width="200" height="50">This
            is a big text!</text>
        <audio url="" loop="true" xCoordinate="250" yCoordinate="100" />
        <line thickness="5" fromX="100" fromY="110" toX="400" toY="130" colour="#FF000069" />
        <image url="" width="300" height="300"
            rotation="69" delay="3" xCoordinate="100"
            yCoordinate="120" timeOnScreen="00:00:20" />
    </slide>
    <slide width="900" height="1000" title="2">
        <video url="https://getsamplefiles.com/download/mp4/sample-5.mp4" loop="true" width="300" height="200"
            xCoordinate="150" yCoordinate="0" />
        <rectangle width="800" height="600" xCoordinate="50" yCoordinate="350" colour="#00FFFFFF"
            borderColour="#FF0000FF" borderWidth="5" shadowRadius="5" shadowDx="5" shadowDy="7"
            shadowColour="#0000FFFF" />
        <circle radius="10" xCoordinate="450" yCoordinate="650" colour="#FF0000FF"
            borderColour="#00FF00FF" borderWidth="10" shadowRadius="15" shadowDx="10" shadowDy="0"
            shadowColour="#FF00FFFF" timeOnScreen="00:01:00" />
        <text fontName="mono" fontSize="32" colour="#FF7B00FF" xCoordinate="300" yCoordinate="250"
            timeOnScreen="00:00:05" width="500" height="100">
            This will disappear in 5 seconds.
        </text>
    </slide>
</presentation>